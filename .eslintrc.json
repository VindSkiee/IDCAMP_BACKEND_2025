{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-underscore-dangle": "off",
    "class-methods-use-this": "off",
    "import/extensions": [
      "error",
      "ignorePackages"
    ],
    "linebreak-style": "off",
    "import/prefer-default-export": "off",
    "newline-per-chained-call": "off"
  }
}
